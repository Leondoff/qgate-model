{
  "name": "basic-account",
  "description": "Account detail with relation to party",
  "kind": "feature-set",
  "spec": {
      "targets": ["parquet"],
      "entities": [
        {
          "name": "account-id",
          "description": "Unique account identificator",
          "type": "string"
        },
        {
          "name": "party-id",
          "description": "Party identificator",
          "type": "string"
        }
      ],
      "features": [
        {
          "name": "account-type",
          "description": "Type of party account (current account, saving account, term account, etc.)",
          "type": "int"
        },
        {
          "name": "account-createdate",
          "description": "Date for account creation",
          "type": "timestamp"
        },
        {
          "name": "account-state",
          "description": "Account state (e.g. active, closed, blocked)",
          "type": "int"
        },
        {
          "name": "record-date",
          "description": "The date when the record was created",
          "type": "timestamp"
        }
      ]
    }
}